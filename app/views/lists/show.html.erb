<div class= "bookmark-header parent" data-controller="new-list">
  <%= link_to "Go back", bookmarks_path, class: "bookmark-form-submit child-left" %>
  <h1 class = "child-middle"><%= @list.name%></h1>
  </div>
<div class="list-div">
  <% @list.bookmarks.each do |bookmark|  %>
    <div class="list-card">
      <%= link_to bookmark.place.name, place_path(bookmark.place), class: "List" %>
      <%= render partial: "components/drop_down_show", locals: {bookmark: bookmark} %>
    </div>
  <% end %>
</div>
